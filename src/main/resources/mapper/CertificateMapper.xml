<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.graduation.mapper.CertificateMapper">

    <resultMap id="BaseResultMap" type="com.graduation.entity.Certificate">
            <id property="id" column="ID" jdbcType="INTEGER"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="file" column="file" jdbcType="VARCHAR"/>
            <result property="obtainDate" column="obtain_date" jdbcType="DATE"/>
            <result property="resumeId" column="resume_id" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        ID,name,file,
        obtain_date,resume_id
    </sql>
</mapper>
